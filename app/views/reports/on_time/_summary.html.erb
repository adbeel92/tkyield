<table class="table table-striped table-responsive">
  <thead>
      <tr>
        <th>Collaborator</th>
        <th>Total Time</th>
      </tr>
  </thead>
  <tbody>
    <% @filtered_users.each do |user| %>
      <tr>
        <% t = TimeStation.total_between_dates(@beginning,@end,user.id) %>
        <td><%= user.full_name %></td>
        <td> <%= get_seconds_in_hours_minutes(t) %> </td>
      </tr>
    <% end %>
  </tbody>
</table>