<div class="col-md-12">
  <table class="table table-striped">
    <thead>
      <tr>
        <th>Name</th>
        <th>Hours</th>            
      </tr>
    </thead>
    <tbody>
      <% @clients.each do |client| %>
        <tr>
          <td><%= link_to client.name, reports_client_path(client) %></td>
          <td><%= get_seconds_in_hours_minutes(client.client_total_time_between_dates(@beginning,@end))%></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>


<div class="align-right">
  <%= link_to '<i class="fa fa-file-excel-o"></i>  Download as Excel'.html_safe, reports_clients_excel_path(format: "xlsx", date: @day_selected), class: "btn btn-lg btn-success"%>
</div>