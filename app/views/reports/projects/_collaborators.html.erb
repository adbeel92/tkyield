<div class="tab-content">
  <div role="tabpanel" class="tab-pane active" id="projects">
    <div class="col-md-12">
      <table class="table table-striped">
        <thead>
          <tr>
            <th>Name </th>
            <th>Hours</th>
          </tr>
        </thead>
        <tbody>
          <% @time.group(:user_id).each do |time| %>
  	        <tr>
  	          <td><%= link_to time.user.full_name, reports_user_path(time.user) %> </td>
  	          <td><%= get_seconds_in_hours_minutes(time.user.total_time_per_project(@project,@beginning,@end)) %></td>
  	        </tr>
  	      <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
