<div class="tab-content">
  <div role="tabpanel" class="tab-pane active" id="projects">
    <div class="col-md-12">
      <table class="table table-striped">
        <thead>
          <tr>
            <th>Name </th>
            <th>Hours</th>
          </tr>
        </thead>
        <tbody>
          <% @users.each do |user| %>
  	        <tr>
  	          <td><%= link_to user.full_name, reports_user_path(user) %> </td>
  	          <td><%= get_seconds_in_hours_minutes(user.total) %></td>
  	        </tr>
  	      <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
