<div class="tab-content">
  <div role="tabpanel" class="tab-pane active" id="projects">
    <div class="col-md-12">
      <table class="table table-striped">
        <thead>
          <tr>
            <th>Name </th>
            <th>Hours</th>
          </tr>
        </thead>
        <tbody>
	        <tr>
	          <td><%= link_to @project.client.name, reports_client_path(@project.client)%> </td>
	          <td><%= get_seconds_in_hours_minutes(@project.project_total_time_between_dates(@beginning,@end)) %></td>
	        </tr>
        </tbody>
      </table>
    </div>
  </div>
</div>
