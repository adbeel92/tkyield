<div class="tab-content">
  <div role="tabpanel" class="tab-pane active" id="projects">
    <div class="col-md-12">
      <table class="table table-striped">
        <thead>
          <tr>
            <th>Name </th>
            <th>Hours</th>
          </tr>
        </thead>
        <tbody>
          <% @project_times.each do |time| %>
  	        <tr>
  	          <td><%= link_to time.project.name, reports_user_project_path(time.user,time.project, :date => date_format_with_hyphen(@day_selected), :type => @type ) %></td>
  	          <td><%= get_seconds_in_hours_minutes(time.user.total_time_per_project(time.project,@beginning,@end)) %></td>
  	        </tr>
  	      <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
