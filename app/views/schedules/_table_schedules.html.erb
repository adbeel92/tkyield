<div class="table-responsive">
  <table class="table table-hover table-striped">
    <thead>
      <tr>
        <th>#</th>
        <th>Name</th>
        <th>Action</th>
      </tr>
    </thead>
    <tbody>  
        <% @schedules.each do |schedule| %>
          <tr id="schedule-row-<%= schedule.id %>">
          	<%= render 'schedule_row',schedule: schedule%>
          </tr>
        <% end %>
    </tbody>
  </table>
</div>