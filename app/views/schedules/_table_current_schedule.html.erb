<div class="field name-schedule-field">
  <%= f.label :name %>
  <%= f.text_field :name, class:"form-control" %>
</div>
<div class=" asd">
  <%= link_to "Add events", '#', class: "btn btn-primary", data:{toggle: "modal", target: "#new-event"} %>
  <%= render 'event_modal', f: f %>
</div>
<table>
  <thead>
    <tr>
      <th>Sunday</th>
      <th>Monday</th>
      <th>Tuesday</th>
      <th>Wednesday</th>
      <th>Thursday</th>
      <th>Friday</th>
      <th>Saturday</th>
    </tr>
  </thead>
  <tbody>
    <%= f.fields_for :events do |day|%>
      <td>
        <div class="field bootstrap-timepicker timepicker">
            <div class="input-group">
              <span class="input-group-addon">In</span>
              <%= day.text_field :inTime,class: "form-control timepicker"  %>
            </div>
            <br>
        </div>
         <div class="field bootstrap-timepicker timepicker">
          <div class="input-group">
            <span class="input-group-addon">Out</span>
            <%= day.text_field :outTime,class: "form-control timepicker"  %>
          </div>
          <br>
        </div>
      </td>
    <%end%>
  </tbody>
</table>

<script type="text/javascript">
  $(".timepicker").timepicker();
</script>